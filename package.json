{
  "name": "@prison-break-idle/main",
  "productName": "prison-break-idle",
  "version": "0.1.0",
  "private": true,
  "description": "My Electron application description",
  "scripts": {
    "start:app": "nx run @prison-break-idle/app:start",
    "build:app": "nx run @prison-break-idle/app:build",
    "start:ds": "nx run @prison-break-idle/design-system:start",
    "build:ds": "nx run @prison-break-idle/design-system:build",
    "package": "nx exec -- electron-forge package",
    "make": "nx exec -- electron-forge make",
    "publish": "nx exec -- electron-forge publish",
    "lint": "nx exec -- eslint --ext .ts,.tsx ."
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [],
  "author": "josh",
  "license": "MIT",
  "devDependencies": {
    "@nrwl/jest": "13.10.6",
    "@nx/react": "16.8.1",
    "@types/jest": "27.0.2",
    "autoprefixer": "10.4.13",
    "electron": "^18.2.3",
    "exitzero": "^1.0.1",
    "jest": "27.2.3",
    "nx": "16.8.1",
    "nx-cloud": "latest",
    "nx-electron": "^13.1.1",
    "postcss": "8.4.21",
    "tailwindcss": "3.2.7",
    "ts-jest": "27.0.5"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "nx": {
    "targets": {
      "package": {
        "outputs": [
          "{projectRoot}/out"
        ]
      },
      "make": {
        "outputs": [
          "{projectRoot}/out"
        ]
      },
      "publish": {
        "outputs": [
          "{projectRoot}/out"
        ]
      }
    }
  }
}
