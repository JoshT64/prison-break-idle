{
  "name": "@prison-break-idle/main",
  "productName": "prison-break-idle",
  "version": "0.1.0",
  "private": true,
  "description": "PBR Description",
  "scripts": {
    "start": "yarn nx run-many --target=start --maxParallel=2",
    "start:app": "nx run @prison-break-idle/app:start",
    "build:app": "nx run @prison-break-idle/app:build",
    "start:ds": "nx run @prison-break-idle/design-system:start",
    "build:ds": "nx run @prison-break-idle/design-system:build",
    "package": "nx run @prison-break-idle/app:package",
    "make": "nx run @prison-break-idle/app:make",
    "publish": "nx run @prison-break-idle/app:publish",
    "lint": "nx exec -- eslint --ext .ts,.tsx ."
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/@prison-break-idle/**"
    ]
  },

  "keywords": [],
  "author": "josh",
  "license": "MIT",
  "devDependencies": {
    "@nrwl/jest": "13.10.6",
    "@nx/react": "16.8.1",
    "@nx/storybook": "^16.9.0",
    "@types/jest": "27.0.2",
    "autoprefixer": "10.4.13",
    "electron": "^18.2.3",
    "exitzero": "^1.0.1",
    "jest": "27.2.3",
    "nx": "16.8.1",
    "nx-cloud": "latest",
    "nx-electron": "^13.1.1",
    "ts-jest": "27.0.5"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "nx": {
    "targets": {
      "package": {
        "outputs": [
          "{projectRoot}/out"
        ]
      },
      "make": {
        "outputs": [
          "{projectRoot}/out"
        ]
      },
      "publish": {
        "outputs": [
          "{projectRoot}/out"
        ]
      }
    }
  }
}
